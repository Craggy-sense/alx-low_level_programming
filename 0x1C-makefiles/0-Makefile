/**
 * Creating my first Makefile
 * Name of the executable school
 * rule: all
 * /

CC=gcc
CFLAGS=-Wall -Werror

all: school


school: main.o fuctions.o
$(CC) $(CFLAGS) -o school main.o functions.o


main.o: main.c functions.h
$(CC) $(CFLAGS) -c main.c

functions.o: function.c functions.h
$(CC) $(CFLAGS) -c functions.c

